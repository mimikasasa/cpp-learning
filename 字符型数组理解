#include<iostream>
using namespace std;

int main()
{
	const char* suit[4] = { "hearts","diamonds","clubs","spades" };

	for (int i = 0; i < 4; i++)
	{
		cout << suit[i] << endl;
	}
	
	//若想要输出地址则
	for (int i = 0; i < 4; i++)
	{
		cout << (void*)suit[i] << endl;//因为字符型数组输出地址编译器会默认输出元素，强转成无类型就会输出地址了
	}

	system("pause");
	return 0;
}
